IDR_MAINFRAME MENU PRELOAD DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New Project...",             ID_FILE_NEW
        MENUITEM "New Project Wizard...",       ID_FILE_WIZARD
        MENUITEM "&Open Project...",            ID_FILE_OPEN
        POPUP "Reopen Project"
        BEGIN
            MENUITEM "Recent File",                 ID_FILE_MRU_FILE1
            , GRAYED
        END
        MENUITEM SEPARATOR
        MENUITEM "&Save Project",               ID_FILE_SAVE
        MENUITEM "Save Proeject &As...",        ID_FILE_SAVE_AS
        MENUITEM "Save All",                    ID_FILE_SAVEALL
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Cu&t\tCtrl+X",                ID_EDIT_CUT
        MENUITEM "&Copy\tCtrl+C",               ID_EDIT_COPY
        MENUITEM "&Paste\tCtrl+V",              ID_EDIT_PASTE
    END
    POPUP "&View"
    BEGIN
        POPUP "Toolbars"
        BEGIN
            MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        END
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
    END
    POPUP "&Project"
    BEGIN
        MENUITEM "&Add Files...",               ID_PROJECT_ADDFILES
        MENUITEM "Import FIles",                ID_PROJECT_IMPORTFILES
        MENUITEM "Export Files",                ID_PROJECT_EXPORTFILES
        MENUITEM SEPARATOR
        MENUITEM "Update All",                  ID_PROJECT_UPDATEALL
        MENUITEM SEPARATOR
        MENUITEM "&Build",                      ID_PROJECT_BUILD
        MENUITEM "Build All",                   ID_PROJECT_BUILDALL
        MENUITEM "Check",                       ID_PROJECT_CHECK
        MENUITEM "Stop",                        ID_PROJECT_STOP
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "Contents",                    ID_HELP_CONTENTS
        MENUITEM "Index",                       ID_HELP_INDEX
        MENUITEM "&About Command Bar Icons Sample...", ID_APP_ABOUT
    END
END


