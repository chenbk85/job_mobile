		MENUITEM "Window List",                 35000
    END
    POPUP "ﾍﾙﾌﾟ\t206"
    BEGIN
        MENUITEM "ﾍﾙﾌﾟのﾄﾋﾟｯｸ",                ID_HELP_FINDER
