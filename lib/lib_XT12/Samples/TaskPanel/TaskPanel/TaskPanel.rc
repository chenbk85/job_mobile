    BEGIN
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
    END
    POPUP "TaskPanel"
    BEGIN
        POPUP "Theme"
        BEGIN
            MENUITEM "Office 2000",                 ID_THEME_OFFICE2000
            MENUITEM "Office 2003",                 ID_THEME_OFFICE2003
            MENUITEM "Native WinXP",                ID_THEME_NATIVEWINXP
            MENUITEM SEPARATOR
            MENUITEM "Office 2000 Plain",           ID_THEME_OFFICE2000PLAIN
            MENUITEM "Office XP Plain",             ID_THEME_OFFICEXPPLAIN
            MENUITEM "Office 2003 Plain",           ID_THEME_OFFICE2003PLAIN
            MENUITEM "Native WinXP Plain",          ID_THEME_NATIVEWINXPPLAIN

            MENUITEM SEPARATOR
            MENUITEM "Custom",                      ID_THEME_CUSTOM
        END
        MENUITEM "Animation",                   ID_TASKPANEL_ANIMATION
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About Task Panel Sample...", ID_APP_ABOUT
    END
END


