<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="mech C/C++"
	>
  <Tool
		Name="VCCLCompilerTool"
		AdditionalOptions="/w44302 /w44286"
		AdditionalIncludeDirectories="$(SolutionDir)..\..\common;$(SolutionDir)..\MOB_lib;$(SVN_ROOT)\common\protobuf;$(SVN_ROOT)\dualgo\Logic\source;$(SVN_ROOT)\lib\lib_protobuf\src;$(SVN_ROOT)\lib\lib_Python\PC;$(SVN_ROOT)\lib\lib_Python\Include;.;$(SRC)\base;$(BOOST)"
		ForceConformanceInForLoopScope="true"
		PreprocessorDefinitions="DEF_WINDOWS;WINDOWS_USE;DEF_DEBUG_ICANDOIT;_CRT_SECURE_NO_WARNINGS;_CRT_NON_CONFORMING_SWPRINTFS;_CRT_SECURE_NO_DEPRECATE;_CRT_NON_CONFORMING_SWPRINTFS;VUTPP_BOOSTTEST"
		RuntimeTypeInfo="false"
		WarningLevel="3"
	/>
  <Tool
		Name="VCLinkerTool"
		AdditionalDependencies="imm32.lib winmm.lib"
		AdditionalLibraryDirectories="$(SVN_ROOT)\lib\lib_python\libs;$(SVN_ROOT)\lib\lib_protobuf\vsprojects\bin\Release;$(BOOST)\lib\windows\$(PlatformName);$(LIB_OUT);VC_$(PlatformName);SDK_$(PlatformName);ATL_$(PlatformName);;$(VCInstallDir)lib;$(VCInstallDir)atlmfc\lib;$(VCInstallDir)atlmfc\lib\i386;$(FrameworkSDKDir)lib;$(VSInstallDir);$(VSInstallDir)lib;"
	/>
  <!--  
  imm32.lib winmm.lib
   -->
  <UserMacro
		Name="VC_Win32"
		Value="$(VCInstallDir)lib"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="VC_x64"
		Value="$(VCInstallDir)lib\amd64"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  
  <UserMacro
		Name="SDK_Win32"
		Value="$(WindowsSdkDir)lib"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SDK_x64"
		Value="$(WindowsSdkDir)lib\x64"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  
  <UserMacro
		Name="ATL_Win32"
		Value="$(VCInstallDir)atlmfc\lib"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="ATL_x64"
		Value="$(VCInstallDir)atlmfc\lib\amd64"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SVN_ROOT"
		Value="$(SolutionDir)..\..\..\.."
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>

  <UserMacro
		Name="SRC"
		Value="$(SolutionDir)..\..\..\lib\windows\mech\src"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="LIB"
		Value="$(SRC)\..\lib\"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="BOOST"
		Value="$(SVN_ROOT)\lib\lib_boost"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="LIB_OUT"
		Value="$(LIB)\mech_rs\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="BIN"
		Value="$(SRC)\..\bin\client\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="OUT"
		Value="o:\mech_rs\$(ProjectName)\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
</VisualStudioPropertySheet>
