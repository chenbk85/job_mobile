<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="mech C/C++"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalOptions="/w44302 /w44286"
		AdditionalIncludeDirectories=".;$(SRC)\base;$(BOOST);$(SRC);$(SRC)\server\TestServer\TestCommon\"
		ForceConformanceInForLoopScope="true"
		PreprocessorDefinitions="_CRT_SECURE_NO_WARNINGS;_CRT_NON_CONFORMING_SWPRINTFS"
		RuntimeTypeInfo="false"
		WarningLevel="3"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="imm32.lib winmm.lib Ws2_32.lib"
		AdditionalLibraryDirectories="$(BOOST)\lib\windows\$(PlatformName);$(LIB_OUT);VC_$(PlatformName)"
	/>
  <!-- ;$(VCInstallDir)PlatformSDK\lib;$(VCInstallDir)lib;$(VCInstallDir)atlmfc\lib;$(VCInstallDir)atlmfc\lib\i386;$(FrameworkSDKDir)lib;$(VSInstallDir);$(VSInstallDir)lib -->
  <UserMacro
  Name="VC_Win32"
  Value="$(WindowsSdkDir)lib;$(VCInstallDir)lib;$(VCInstallDir)atlmfc\lib"
  InheritsFromParent="true"
  Delimiter=";"
  PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="VC_x64"
		Value="$(WindowsSdkDir)lib\x64;$(VCInstallDir)lib\amd64;$(VCInstallDir)atlmfc\lib\amd64"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
  Name="SVN"
  Value="$(SolutionDir)..\..\..\..\"
  InheritsFromParent="true"
  Delimiter=";"
  PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SRC"
		Value="$(SolutionDir).."
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="LIB"
		Value="$(SRC)/../lib"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="BOOST"
		Value="$(SVN)\lib\lib_boost"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="LIB_OUT"
		Value="$(LIB)\mech_rs\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="BIN"
		Value="$(SRC)\..\bin\client\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SERVER_BIN"
		Value="$(BIN)\..\server\bin\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="OUT"
		Value="o:\mech_rs\AqServer\$(ProjectName)\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
	
</VisualStudioPropertySheet>
